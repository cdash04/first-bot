{
    "name": "first-bot",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "bootstrap": "lerna bootstrap",
        "build": "lerna run build",
        "dev": "lerna run dev",
        "start": "lerna run start",
        "build-layer": "lerna run build-layer",
        "deploy:dev": "lerna run deploy:dev",
        "test": "jest --coverage=false --verbose",
        "lint": "eslint packages/**/*.ts",
